<%= form_with model: Song.new, url: songs_path, multipart: true do |f| %>
  <%= f.file_field :audio_file, value: "", class: "mb-5 lavender-3"%>

  <h6 class="text-start">TAG IT</h6>
  <%= f.text_field :tag_list,
      required: true,
      autofocus: true,
      class: "form-control text-box white"%>
  <div class="mt-3 btn clear-btn">
    <%= f.submit "Add song", class: "btn white" %>
  </div>
<% end %>
