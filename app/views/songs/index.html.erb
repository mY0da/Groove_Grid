<div class="flex-container">
  <div class ="song-table-container">
    <table>
      <thead>
        <tr>
          <th>Song</th>
          <th>BPM</th>
          <th>Artist</th>
          <th>Scale</th>
          <th>Duration</th>
          <th>Tags</th>
        </tr>
      </thead>
      <tbody>
      <% @songs.each do |song| %>
        <tr data-action="click->music-player#select" data-song="<%= song.to_json %>" data-url="<%= cl_video_path song.audio_file.key || '' %>">
          <td><%= song.name %></td>
          <td><%= song.bpm %></td>
          <td><%= song.artist.name %></td>
          <td><%= song.scale %></td>
          <td><%= song.seconds %></td>
          <td><%= song.tags.name %></td>
        </tr>
      <% end %>
      <tbody>
    </table>
  </div>
</div>
