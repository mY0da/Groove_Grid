<div class="flex-container">
  <div class ="song-table-container">
    <table>
      <thead>
        <tr>
          <th>Song</th>
          <th>BPM</th>
          <th>Artist</th>
          <th>Album</th>
          <th>Scale</th>
          <th>Duration</th>
          <th>Tags</th>
        </tr>
      </thead>
      <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.name %></td>
          <td><%= song.bpm %></td>
          <td><%= song.artist %></td>
          <td><%= song.album %> </td>
          <td><%= song.scale %></td>
          <td><%= song.duration %></td>
          <td><%= song.tags %></td>
        </tr>
      <% end %>
      <tbody>
    </table>
  </div>
</div>
