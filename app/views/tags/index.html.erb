<div class="container">

  <h1>Your tags</h1>
  <%= render "/shared/audio_player"%>
  <p>
    <div class="search-bar">
      <input type="text" placeholder="Search...">
    </div>
  </p>

  <p>
    <div class ="song-table-container">
      <table>
        <tbody>
          <% @tags.each do |tag| %>
            <tr>
              <td class="grey">
                <%= link_to tag_path(tag) do %>
                  <span class="song-tag"><%= tag.name %></span>
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </p>
</div>
